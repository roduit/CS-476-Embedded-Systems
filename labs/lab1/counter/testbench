    .scope S_0x5593858773a0;
T_1 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55938588d560_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55938588d4a0_0, 0, 1;
    %pushi/vec4 4, 0, 32;
T_1.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_1.1, 5;
    %jmp/1 T_1.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 5, 0;
    %load/vec4 v0x55938588d4a0_0;
    %inv;
    %store/vec4 v0x55938588d4a0_0, 0, 1;
    %jmp T_1.0;
T_1.1 ;
    %pop/vec4 1;
    %pushi/vec4 0, 0, 1;
