S_0x557a749f7530 .scope module, "sr_latch_tb" "sr_latch_tb" 2 2;
 .timescale 0 0;
v0x557a74a22ab0_0 .net "q", 0 0, L_0x557a74a23040;  1 drivers
v0x557a74a22b70_0 .net "q_not", 0 0, L_0x557a74a23240;  1 drivers
v0x557a74a22c10_0 .var "r", 0 0;
v0x557a74a22cb0_0 .var "s", 0 0;
S_0x557a749fb7d0 .scope module, "latch" "sr_latch" 2 7, 3 4 0, S_0x557a749f7530;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "S";
    .port_info 1 /INPUT 1 "R";
    .port_info 2 /OUTPUT 1 "Q";
    .port_info 3 /OUTPUT 1 "Qn";
L_0x557a74a22d80 .functor NOT 1, v0x557a74a22c10_0, C4<0>, C4<0>, C4<0>;
L_0x557a74a22e70 .functor AND 1, L_0x557a74a22d80, v0x557a74a22cb0_0, C4<1>, C4<1>;
L_0x557a74a22f80 .functor OR 1, v0x557a74a22c10_0, L_0x557a74a23240, C4<0>, C4<0>;
L_0x557a74a23040 .functor NOT 1, L_0x557a74a22f80, C4<0>, C4<0>, C4<0>;
L_0x557a74a23180 .functor OR 1, L_0x557a74a22e70, L_0x557a74a23040, C4<0>, C4<0>;
L_0x557a74a23240 .functor NOT 1, L_0x557a74a23180, C4<0>, C4<0>, C4<0>;
v0x557a74a09c30_0 .net "Q", 0 0, L_0x557a74a23040;  alias, 1 drivers
v0x557a74a22460_0 .net "Qn", 0 0, L_0x557a74a23240;  alias, 1 drivers
v0x557a74a22520_0 .net "R", 0 0, v0x557a74a22c10_0;  1 drivers
v0x557a74a225c0_0 .net "S", 0 0, v0x557a74a22cb0_0;  1 drivers
v0x557a74a22680_0 .net "Sp", 0 0, L_0x557a74a22e70;  1 drivers
v0x557a74a22790_0 .net *"_ivl_0", 0 0, L_0x557a74a22d80;  1 drivers
v0x557a74a22870_0 .net *"_ivl_4", 0 0, L_0x557a74a22f80;  1 drivers
v0x557a74a22950_0 .net *"_ivl_8", 0 0, L_0x557a74a23180;  1 drivers
    .scope S_0x557a749f7530;
T_0 ;
    %vpi_call 2 15 "$dumpfile", "sr_latch.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", v0x557a74a22cb0_0, v0x557a74a22c10_0, v0x557a74a22ab0_0, v0x557a74a22b70_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x557a74a22cb0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x557a74a22c10_0, 0, 1;
    %delay 1, 0;
    %vpi_call 2 20 "$display", "s=%b, r=%b ==> q=%b, q_not=%b # should be undefined", v0x557a74a22cb0_0, v0x557a74a22c10_0, v0x557a74a22ab0_0, v0x557a74a22b70_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x557a74a22cb0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x557a74a22c10_0, 0, 1;
    %delay 1, 0;
    %vpi_call 2 25 "$display", "s=%b, r=%b ==> q=%b, q_not=%b # reset, so q=0", v0x557a74a22cb0_0, v0x557a74a22c10_0, v0x557a74a22ab0_0, v0x557a74a22b70_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x557a74a22cb0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x557a74a22c10_0, 0, 1;
    %delay 1, 0;
