    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x557a74a22cb0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x557a74a22c10_0, 0, 1;
    %delay 1, 0;
    %vpi_call 2 35 "$display", "s=%b, r=%b ==> q=%b, q_not=%b # latch, so q=q (keep state)", v0x557a74a22cb0_0, v0x557a74a22c10_0, v0x557a74a22ab0_0, v0x557a74a22b70_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x557a74a22cb0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x557a74a22c10_0, 0, 1;
    %delay 1, 0;
